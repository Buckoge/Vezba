@model IEnumerable<Product>

    <link href="~/css/main.css" rel="stylesheet" />

<p>Total: @Model.Count()</p>


    <table>
        <thead>
            <tr>
                <td>Name</td>
                <td>Category</td>
                <td>Price</td>
            </tr>
        </thead>
        <tbody class="row100">
            @foreach (var t in Model)
            {
                <tr>
                    <td>@t.Title</td>
                    <td>@t.Category</td>
                    <td>@t.Price</td>
                </tr>
            }
        </tbody>
    </table>    
